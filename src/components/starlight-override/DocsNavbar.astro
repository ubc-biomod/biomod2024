---
import type { NavbarProps } from "./types";

export const props: NavbarProps = { links: [] }; 
const { links } = props;
---

<nav class="docs-navbar">
  <div class="logo">
    <a href="/">Docs</a>
  </div>
  <div class="nav-links">
    {links.map(link => (
      <a href={link.href}>{link.title}</a>
    ))}
  </div>
</nav>

<style>
  .docs-navbar {
    /* Mobile-friendly and minimal styles */
  }
</style>
